# Zaciatok main
LABEL $$main
CREATEFRAME
PUSHFRAME

# Built-in function Len
LABEL $function_len
PUSHFRAME
DEFVAR LF@%navratova_hodnota
STRLEN LF@%navratova_hodnota LF@%0
POPFRAME
